{
  "name": "book-store",
  "version": "1.0.0",
  "description": "A book store to find all genres of book you need",
  "main": "index.js",
  "scripts": {
    "compile:sass": "sass --watch --no-source-map sass/main.scss:css/style.css",
    "autoprefixer": "postcss css/style.css --use autoprefixer -d build/",
    "build:file": "npm run compile:sass && npm run autoprefixer"
  },
  "author": "Kunga Tashi",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "postcss-cli": "^8.3.1"
  },
  "keywords": [
    "book",
    "showcase"
  ],
  "engines": {
    "node": "14.15.4"
  }
}
